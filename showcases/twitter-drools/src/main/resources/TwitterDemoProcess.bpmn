      <incoming>SequenceFlow_2</incoming>
      <outgoing>SequenceFlow</outgoing>
    </businessRuleTask>
    <sequenceFlow id="SequenceFlow" name="" sourceRef="BusinessRuleTask_1" targetRef="gateway_approved"/>
